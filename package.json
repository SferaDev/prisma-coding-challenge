{
    "private": true,
    "name": "prisma-coding-challenge",
    "description": "",
    "license": "",
    "scripts": {
        "build": "remix build",
        "dev": "concurrently \"remix dev\" \"tsc --watch\"",
        "postinstall": "remix setup node && prisma generate",
        "start": "remix-serve build",
        "prettify": "prettier --write '**/*.{js,ts,tsx,json,md,yml}'",
        "lint": "eslint app --ext .js,.jsx,.ts,.tsx",
        "pretest": "dotenv -e .env.test -- prisma db push",
        "test": "jest --passWithNoTests",
        "posttest": "dotenv -e .env.test -- prisma migrate reset --force",
        "docker:up": "docker-compose up -d",
        "docker:down": "docker-compose down",
        "docker:test:up": "docker-compose --env-file=.env.test up -d",
        "docker:test:down": "docker-compose --env-file=.env.test down",
        "db:seed": "node --require esbuild-register prisma/seed.ts",
        "db:clear": "prisma migrate reset",
        "db:push": "prisma db push"
    },
    "prisma": {
        "seed": "yarn db:seed"
    },
    "dependencies": {
        "@prisma/client": "3.8.0",
        "@remix-run/react": "1.1.1",
        "@remix-run/serve": "1.1.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "remix": "1.1.1",
        "swagger-ui": "4.1.3",
        "tiny-invariant": "1.2.0",
        "zod": "3.11.6"
    },
    "devDependencies": {
        "@remix-run/dev": "1.1.1",
        "@types/jest": "27.4.0",
        "@types/react": "17.0.38",
        "@types/react-dom": "17.0.11",
        "@types/swagger-schema-official": "2.0.22",
        "@types/swagger-ui": "3.52.0",
        "@typescript-eslint/eslint-plugin": "5.9.1",
        "@typescript-eslint/parser": "5.9.1",
        "concurrently": "7.0.0",
        "dotenv": "12.0.3",
        "dotenv-cli": "4.1.1",
        "esbuild-jest": "0.5.0",
        "esbuild-register": "3.3.2",
        "eslint": "8.6.0",
        "eslint-config-react-app": "7.0.0",
        "eslint-plugin-primer-react": "0.7.3",
        "eslint-plugin-unused-imports": "2.0.0",
        "jest": "27.4.7",
        "jest-fetch-mock": "3.0.3",
        "jest-mock-extended": "2.0.4",
        "prisma": "3.8.0",
        "ts-jest": "27.1.2",
        "ts-node": "10.4.0",
        "typescript": "4.5.4"
    },
    "engines": {
        "node": ">=14"
    },
    "sideEffects": false
}
